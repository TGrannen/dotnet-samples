{
  "AllowedHosts": "*",
  "FlakyServerUri": "https://localhost:5011",
  "Resiliency": {
    "RetryOptions": {
      "Retry": {
        "BackoffType": "Exponential",
        "UseJitter": false,
        "Delay": "00:00:01",
        "MaxRetryAttempts": 7
      }
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Default": "Information",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": {
      "ConsoleSink": {
        "Name": "Console"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "Properties": {
      "Application": "ConsoleApp_Microservice"
    }
  }
}