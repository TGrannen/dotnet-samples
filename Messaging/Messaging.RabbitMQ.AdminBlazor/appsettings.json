{
  "RabbitMQ": {
    "Host": "localhost"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Default": "Information",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Warning"
      }
    },
    "WriteTo": {
      "Console": {
        "Name": "Console"
      },
      "Seq": {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5555"
        }
      }
    },
    "Properties": {
      "ApplicationName": "RabbitMQ-Blazor"
    }
  },
  "AllowedHosts": "*"
}
