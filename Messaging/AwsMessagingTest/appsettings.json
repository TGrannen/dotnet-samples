{
  "AllowedHosts": "*",
  "TestingConfig": {
    "RateOfChat": "00:00:01",
    "BackoffDelay": "00:00:00",
    "Chat": {
      "Delay": "00:00:00",
      "Throw": false,
      "ReturnFailure": false
    },
    "Order": {
      "Delay": "00:00:01",
      "Throw": false,
      "ReturnFailure": false
    },
    "ShouldThrowChat": false,
    "ShouldThrowOrder": false
  },
  "Queues": {
    "ChatUrl": "TODO",
    "OrderInfoUrl": "TODO"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Default": "Debug",
        "MassTransit": "Debug",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Polly": "Warning",
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "WriteTo": {
      "ConsoleSink": {
        "Name": "Console"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ]
  }
}
