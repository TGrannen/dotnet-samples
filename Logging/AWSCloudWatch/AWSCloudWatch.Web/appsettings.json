{
  "AWS": {
    "MinimumLevel": "Information",
    "LogToCloudWatch": true,
    "LogGroup": "dotnet-samples",
    "Region": "us-east-1",
    "RetentionPolicy": "SixMonths",
    "AccessKey": "<TODO>",
    "SecretKey": "<TODO>"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Information",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "AllowedHosts": "*"
}